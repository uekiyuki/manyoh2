<h1>ログイン</h1>

<%= form_with(scope: :session, local: true) do |form| %>

  <%= form.label :email %>
  <%= form.text_field :email, class: 'form-control', id: 'session_email' %>



  <%= form.label :password %>
  <br>
  <%= form.password_field :password, class: 'form_control', id: 'session_password' %>
    <br>

  <%= form.submit "ログインする", class: 'btn btn-primary' %>

<%end %>